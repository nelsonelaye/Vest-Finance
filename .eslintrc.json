{
  "extends": ["next/core-web-vitals", "eslint:recommended"],
  "rules": {
    "no-use-before-define": ["error", { "variables": false }],
    "no-undef": "error",
    "no-console": "warn",
    "no-unused-expressions": "error",
    "no-unused-vars": "warn",
    "react/prop-types": "off",
    "@next/next/no-img-element": "off",
    "react-hooks/exhaustive-deps": "off"
  }
}
